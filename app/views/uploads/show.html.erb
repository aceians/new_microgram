<br>


<h1 style="text-align:center">This Submission</h1>
<br>
    <div  style="overflow:auto">   
      <% @upload.images.all.each do |a| %>
    <div class="col-sm-5 submission_card" style="height:160px; width:160px">
      <%= image_tag a.image.url(:small) %>
    </div>
      <% end %>
    </div>
    <br>
    <div>     
          <p>
            <li class="tags"> Tags:  </li>
          <% @upload.tags.all.each do |b| %>
              <li class="tags">  <%= link_to b.tagname, uploads_search_path(:search => b.tagname) %></li>
          <% end %>
          </p> 
          

    
    <p class="tagsss"> Description : <%= @upload.description %> </p>
    <p class="tagsss"> Created by : <%= @upload.user.name %> (<%= @upload.user.email %> )  </p>
    <p class="tagsss"> Permission : <%= @upload.permission %></p>
    
    <br>


       

		  
		  <br><br>
		  <% if current_user == @upload.user %>
		  <h4>	<%= link_to "Edit", edit_upload_path(@upload) %></h4>
		  <% end %>
    </div>



